<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.co.cinemuadmin.dao.AdminUploadDao">
  
  <insert id="snackUp" parameterType="snackupvo">
  insert into snack values(snack_seq.nextVal,#{poppay},#{popimg},#{popname},#{usedate},#{country},#{snackdetail})
  </insert>
  
  <insert id="movieUp" parameterType="movieupvo">
  insert into movieup values(movieup_seq.nextVal,#{mimg},#{mname},#{msaram},#{mtype},#{mtime},#{mage},#{mstartday},#{mdirec},#{mstory},#{mactor},#{mendday},#{mmain})
  </insert>
  
  </mapper>